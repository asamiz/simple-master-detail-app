    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "lint-staged": "^13.0.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
